#include <stdio.h>
#include <stdlib.h>

int main(void) 
{	
	int vet[] = {4,9,13};
    int i;
    for(i=0;i<3;i++)
    {
        printf("%d ",*(vet+i));
    }
	 //imprime os elementos do vetor vet

   int vet[] = {4,9,13};
    int i;
    for(i=0;i<3;i++)
    {
        printf("%x ",vet+i);
    }
	 //imprime os enderecos de cada elemento do vetor vet em hexadecimal
    
  return 0;
}
